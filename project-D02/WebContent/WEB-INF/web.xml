<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
  <display-name>project-D02</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

	<resource-ref>
		<res-ref-name>jdbc/form</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>


  <servlet>
    <description></description>
    <display-name>AfficheServlet</display-name>
    <servlet-name>AfficheServlet</servlet-name>
    <servlet-class>com.controller.AfficheServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AfficheServlet</servlet-name>
    <url-pattern>/affiche</url-pattern>
  </servlet-mapping>


</web-app>